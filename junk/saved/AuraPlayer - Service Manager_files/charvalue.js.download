function charValue(charCode) {

    if (charCode == undefined || charCode == "") return "";

    var value = String.fromCharCode(charCode);
    if (charCode == 8) value = "Backspace"; //  backspace
    if (charCode == 9) value = "Tab"; //  tab
    if (charCode == 13) value = "Enter"; //  enter
    if (charCode == 16) value = "Shift"; //  shift
    if (charCode == 17) value = "Ctrl"; //  ctrl
    if (charCode == 18) value = "Alt"; //  alt
    if (charCode == 19) value = "Pause/Break"; //  pause/break
    if (charCode == 20) value = "Caps Lock"; //  caps lock
    if (charCode == 27) value = "Escape"; //  escape
    if (charCode == 33) value = "Page Up"; // page up, to avoid displaying alternate character and confusing people             
    if (charCode == 34) value = "Page Down"; // page down
    if (charCode == 35) value = "End"; // end
    if (charCode == 36) value = "Home"; // home
    if (charCode == 37) value = "Left Arrow"; // left arrow
    if (charCode == 38) value = "Up Arrow"; // up arrow
    if (charCode == 39) value = "Right Arrow"; // right arrow
    if (charCode == 40) value = "Down Arrow"; // down arrow
    if (charCode == 45) value = "Insert"; // insert
    if (charCode == 46) value = "Delete"; // delete
    if (charCode == 91) value = "Left Window"; // left window
    if (charCode == 92) value = "Right Window"; // right window
    if (charCode == 93) value = "Select Key"; // select key
    if (charCode == 96) value = "Numpad 0"; // numpad 0
    if (charCode == 97) value = "Numpad 1"; // numpad 1
    if (charCode == 98) value = "Numpad 2"; // numpad 2
    if (charCode == 99) value = "Numpad 3"; // numpad 3
    if (charCode == 100) value = "Numpad 4"; // numpad 4
    if (charCode == 101) value = "Numpad 5"; // numpad 5
    if (charCode == 102) value = "Numpad 6"; // numpad 6
    if (charCode == 103) value = "Numpad 7"; // numpad 7
    if (charCode == 104) value = "Numpad 8"; // numpad 8
    if (charCode == 105) value = "Numpad 9"; // numpad 9
    if (charCode == 106) value = "Multiply"; // multiply
    if (charCode == 107) value = "Add"; // add
    if (charCode == 109) value = "Subtract"; // subtract
    if (charCode == 110) value = "Decimal point"; // decimal point
    if (charCode == 111) value = "Divide"; // divide
    if (charCode == 112) value = "F1"; // F1
    if (charCode == 113) value = "F2"; // F2
    if (charCode == 114) value = "F3"; // F3
    if (charCode == 115) value = "F4"; // F4
    if (charCode == 116) value = "F5"; // F5
    if (charCode == 117) value = "F6"; // F6
    if (charCode == 118) value = "F7"; // F7
    if (charCode == 119) value = "F8"; // F8
    if (charCode == 120) value = "F9"; // F9
    if (charCode == 121) value = "F10"; // F10
    if (charCode == 122) value = "F11"; // F11
    if (charCode == 123) value = "F12"; // F12
    if (charCode == 144) value = "Num Lock"; // num lock
    if (charCode == 145) value = "Scroll Lock"; // scroll lock
    if (charCode == 186) value = ";"; // semi-colon
    if (charCode == 187) value = "="; // equal-sign
    if (charCode == 188) value = ","; // comma
    if (charCode == 189) value = "-"; // dash
    if (charCode == 190) value = "."; // period
    if (charCode == 191) value = "/"; // forward slash
    if (charCode == 192) value = "`"; // grave accent
    if (charCode == 219) value = "["; // open bracket
    if (charCode == 220) value = "\\"; // back slash
    if (charCode == 221) value = "]"; // close bracket
    if (charCode == 222) value = "'"; // single quote


    if (value.length > 0 && charCode != undefined) return value;
    else return "";
}
